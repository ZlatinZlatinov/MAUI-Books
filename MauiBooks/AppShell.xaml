<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiBooks.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiBooks.Pages"
    Title="MauiBooks">

    <TabBar>
    
    <ShellContent 
        Title="Test"
        ContentTemplate="{DataTemplate local:TestPage}"
        Route="Test"/>

    <ShellContent 
        Title="Add Book"
        ContentTemplate="{DataTemplate local:AddBook}"
        Route="AddBook"/>

        <ShellContent 
         Title="Catalog"
         ContentTemplate="{DataTemplate local:Catalog}"
         Route="Catalog" />
    </TabBar>
</Shell>
